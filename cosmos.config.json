  "staticPath": "public",
  "watchDirs": [
    "src"
  ],
  "webpack": {
    "configPath": "react-scripts/config/webpack.config",
    "includeHashInOutputFilename": true
  },
  "plugins": [
    "react-cosmos-plugin-webpack"
  ],
  "globalImports": [
